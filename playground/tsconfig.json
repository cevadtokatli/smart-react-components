    "downlevelIteration": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es2015"
  },
  "include": ["src/**/*.tsx"],
  "exclude": ["node_modules"]
}
